{
  "name": "@guestbell/fast-ftp",
  "version": "1.0.0",
  "description": "Supercharge your ftp deployments",
  "main": "index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "api-docs": "typedoc --plugin typedoc-plugin-markdown --out api-docs src/lib.ts",
    "build": "rollup -c",
    "version:patch": "npm version patch",
    "push:git": "git push --tags origin main --quiet",
    "commit:build": "git add . && git commit -m \"Deploy commit\" --quiet && npm run push:git || true",
    "create:patch": "npm run version:patch && npm run push:git",
    "deploy": "npm run api-docs && npm run build && npm run commit:build && npm publish && npm run create:patch"
  },
  "keywords": [
    "ftp",
    "static",
    "fast"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guestbell/fast-ftp.git"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/babel__core": "^7.20.0",
    "@types/ftp": "^0.3.33",
    "@types/minimist": "^1.2.2",
    "babel-loader": "^8.3.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "rollup": "^3.11.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.23.24",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  },
  "dependecies": {
    "dotenv": "^16.0.3",
    "ftp": "^0.3.10",
    "minimist": "^1.2.8"
  },
  "author": "Peter Kottas",
  "license": "MIT",
  "dependencies": {
    "ftp": "^0.3.10"
  }
}
